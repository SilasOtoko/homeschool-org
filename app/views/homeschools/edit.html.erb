<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <div class="custom-well">
      <div class='form-container'>
        <%= bootstrap_form_for @homeschool do |f| %>
          <%= render 'shared/errors', obj: @homeschool %>
            <%= f.text_field :name %>
          <%= f.submit 'Update', class: 'btn btn-success' %>
        <% end %>
      </div>
    </div>
  </div>
</div>

